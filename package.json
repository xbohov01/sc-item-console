{
  "name": "item-console",
  "version": "1.0.0",
  "description": "csv based item lookup for SC",
  "main": "./lib/index.js",
  "bin": {
    "item-console": "./lib/index.js"
  },
  "scripts": {
    "dev": "npm run build && npm run start",
    "start": "nodemon --watch 'src/**/*.ts' --exec ts-node src/index.ts",
    "create": "npm run build && npm run test",
    "build": "tsc -p .",
    "test": "npm i -g && item-console",
    "refresh": "rm -rf ./node_modules ./package-lock.json && npm install",
    "package": "pkg . --targets node10-win-x64 --out-path \\builds"
  },
  "author": "TheSpaceCoder",
  "license": "ISC",
  "dependencies": {
    "chalk": "^4.0.0",
    "clear": "^0.1.0",
    "commander": "^5.0.0",
    "csv-parse": "^4.8.9",
    "figlet": "^1.3.0",
    "fs": "0.0.1-security",
    "path": "^0.12.7",
    "readline-sync": "^1.4.10"
  },
  "pkg": {
    "assets": [
    "./node_modules/chalk/**",
    "./node_modules/clear/**",
    "./node_modules/csv-parse/**",
    "./node_modules/figlet/**",
    "./node_modules/fs/**"
    ]
    },
  "devDependencies": {
    "@types/clear": "^0.1.0",
    "@types/figlet": "^1.2.0",
    "@types/node": "^13.13.2",
    "@types/readline-sync": "^1.4.3",
    "nodemon": "^2.0.3",
    "ts-node": "^8.9.0",
    "typescript": "^3.8.3"
  }
}
